# Getting Proxy Template
wget -q -O /etc/xtrasocket https://raw.githubusercontent.com/bannerpy/Files/main/xtrasocket.py
chmod +x /etc/xtrasocket

service

function service1() {
# Installing Service
cat << END > /etc/systemd/system/xtrasocket.service 
[Unit]
Description=xtrasocket
Documentation=https://google.com
After=network.target nss-lookup.target

[Service]
Type=simple
User=root
CapabilityBoundingSet=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
AmbientCapabilities=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
NoNewPrivileges=true
ExecStart=/usr/bin/python -O /etc/xtrasocket
Restart=on-failure

[Install]
WantedBy=multi-user.target
END
}
service1

function setting() {
sed -i "/DEFAULT_HOST = '127.0.0.1:443'/c\DEFAULT_HOST = '127.0.0.1:110'" /etc/xtrasocket

systemctl daemon-reload
systemctl enable xtrasocket
systemctl restart xtrasocket
}
setting

function remove() {
echo "" > .bash_history 
history -c
echo '' > /var/log/syslog
}
remove

clear

wget -q 'https://github.com/Projectmarkallen/projectmark/raw/main/Antitorrentado.sh' && chmod +x Antitorrentado.sh && ./Antitorrentado.sh

clear

rm /root/.bash_history
echo 'UTUT MU' > /root/.bash_history

echo "꧁OVPN WS INSTALLED꧂"
echo "꧁ANTI TORRENT INSTALLED꧂"
echo "꧁MARK KING DURAN SCRIPT꧂"
